<section id="blog-page">
<div class="container">
<div class="row">
<div id="blog-posts" class="col-md-8 col-sm-8">
<div class="post-item">
<!-- Single Post -->
<% for(var i = 0 ; i < data.length ; i++){%>
<article class="entry wow fadeInDown"  data-wow-duration="1000ms" data-wow-delay="300ms">
                    <div class="post-thumb">

<a href="/getAdmin/<%= data[i]._id %>"></a>
<img src="img/blog/3D-beach-art.jpg" alt="3D Beach Art | Meghna" class="img-responsive">
</div>
<div class="post-excerpt">
<h3><a href="/getQuery/mid/<%= data[i].qid._id %>"><%= data[i].qid.heading %></a></h3>
<p>Reply:-<%= data[i].reply %></p>
<a class="btn btn-transparent" href="/getQuery/mid/<%=data[i].qid._id %>">Read more</a>
</div>
<div class="post-meta">
<span class="post-date">
<i class="fa fa-calendar"></i><%= data[i].qid.createdAt %>
</span>
<span class="post-tags">
<i class="fa fa-tags"></i><a href="/getAdmin/tags/<%=data[i].qid.tags %>"><%=data[i].qid.tags%></a>
</span>

</div>
</article>
<%}%>
<!-- End Single Post -->
